// https://code.visualstudio.com/api/references/theme-color

{
    "name": "Valatos Theme Dark",
    "type": "dark",
    "colors": {
        "focusBorder": "#c2436d",
        "foreground": "#d3d3d3",

        "selection.background": "#c2436d5b",

        "editor.background": "#202020",
        "editor.foreground": "#d3d3d3",

        "editor.findMatchBackground": "#88f09142",
        "editor.findMatchHighlightBackground": "#e47d704d",

        "editor.selectionBackground": "#c2436d5b",
        "minimap.selectionHighlight": "#c2436d5b",

        "textLink.foreground": "#c2436d",
        "textLink.activeForeground": "#943253",

        "statusBar.background": "#4b4b4b",
        "statusBar.noFolderBackground": "#4b4b4b",

        "button.background": "#c2436d",
        "button.foreground": "#d3d3d3",
        "button.hoverBackground": "#97405d",

        "dropdown.background": "#303030",
        "dropdown.foreground": "#d3d3d3",
        "dropdown.border": "#454545",
        "dropdown.listBackground": "#454545",

        "activityBar.background": "#333333",
        "activityBar.foreground": "#d3d3d3",
        "activityBar.border": "#454545",
        "activityBar.activeBorder": "#c2436d",

        "activityBarBadge.background": "#c2436d",
        "activityBarBadge.foreground": "#d3d3d3",

        "sideBar.background": "#2b2b2b",
        "sideBar.border": "#454545",
        "sideBar.foreground": "#d3d3d3",
        "sideBar.dropBackground": "#a3a3a33f",

        "sideBarTitle.foreground": "#bbbbbb",

        "sideBarSectionHeader.background": "#404040",
        "sideBarSectionHeader.border": "#4e4e4e",
        "sideBarSectionHeader.foreground": "#d3d3d3",

        "editorGroup.border": "#454545",
        "editorGroup.dropBackground": "#a3a3a325",
        "editorGroup.emptyBackground": "#202020",
        "editorGroup.focusedEmptyBorder": "#c2436d",

        "editorGroupHeader.border": "#303030",
        "editorGroupHeader.noTabsBackground": "#272727",
        "editorGroupHeader.tabsBackground": "#272727",

        "list.hoverBackground": "#3a3a3a",
        "list.activeSelectionBackground": "#464646",
        "list.focusOutline": "#2b2b2b",
        "list.focusBackground": "#c2436d5b",
        "list.highlightForeground": "#c2436d",

        "pickerGroup.foreground": "#c2436d",

        "tab.activeBackground": "#202020",
        "tab.activeForeground": "#d3d3d3",
        "tab.inactiveBackground": "#2e2e2e",
        "tab.hoverBackground": "#3b3b3b",

        "panel.background": "#2b2b2b",
        "panel.border": "#454545",

        "settings.modifiedItemIndicator": "#c2436d",

        "checkbox.background": "#404040",

        "progressBar.background": "#c2436d",

        "titleBar.activeBackground": "#333333",
        "titleBar.inactiveBackground": "#333333",

        "editor.hoverHighlightBackground": "#c2436d27"
    },
    "tokenColors": [
        {
            "name": "Color 1",
            "scope": [
                "storage.type",
                "storage.type.function",
                "storage.modifier",
                "storage.type.class",
                "storage.type.interface",
                "storage.type.namespace",
                "storage.type.struct",
                "storage.type.enum",
                "keyword.operator.type",
                "keyword.operator.new",
                "keyword.operator.delete",
                "keyword.control",
                "keyword.other",
                "keyword.operator.expression",
                "entity.name.tag",
                "variable.language.self",
                "variable.language.this",
                "variable.language.super",

                // "entity.name.tag.html",
                // "entity.name.tag.css",
                "keyword.operator.logical.php",
                "keyword.cmake",
                "support.type.property-name.json",
                "storage.type.js",
                "storage.type.ts",
                "storage.type.rust",
                "entity.name.section.markdown",
                "entity.other.attribute-name.table.toml",
                "meta.tag.table.toml"
            ],
            "settings": {
                "foreground": "#c2436d"
            }
        },
        {
            "name": "Color 2",
            "scope": [
                "keyword.operator.sizeof",
                "entity.other.inherited-class",
                "entity.name.function.preprocessor",
                "entity.name.scope-resolution",
                "entity.name.other.preprocessor.macro",
                "storage.type.annotation",
                // "entity.name.namespace",
                "storage.type.generic",
                "constant.character.escape",

                "entity.source.cmake",
                "meta.property-name.css",
                // "support.type.property-name.css",
                "constant.other.class.php",
                "markup.underline.link.markdown",
                "keyword.key.toml"
            ],
            "settings": {
                "foreground": "#b0a2e0",
            }
        },
        {
            "name": "Color 3",
            "scope": [
                // "variable.other.object",

                "meta.property-value.css",
                "punctuation.definition.constant.css",
                "punctuation.section.embedded.begin.php",
                "punctuation.section.embedded.end.php",
                "variable.other.global.php",
                "variable.other.global.safer.php",
                "entity.name.function.macro.rust",
                "support.constant.json",
                "meta.macro.rust"
            ],
            "settings": {
                "foreground": "#4788bd"
            }
        },
        {
            "name": "Color 4",
            "scope": [
                // "meta.function-call",
                "entity.name.function",
                "support.function",
                "entity.other.attribute-name",

                "meta.selector.css",
                "meta.function.url.css",
                "support.other.namespace.php",
                "support.function.construct.output.php",
                "keyword.other.type.php",
                "storage.modifier.package.java",
                "storage.modifier.import.java",
                "storage.source.cmake",
                "markup.bold.markdown",
                "markup.italic.markdown",
                "variable.source.cmake",
                "variable.language.documentroot.xml",
                "support.function.json.ts"
            ],
            "settings": {
                "foreground": "#7d91af",
            }
        },
        {
            "name": "Color 5",
            "scope": [
                "constant.numeric",
                "constant.language",
                "storage.type",
                // "storage.type.built-in",
                // "storage.type.built-in.primitive",
                "support.class",
                "support.type",
                "support.type.builtin",
                "support.type.primitive",
                "keyword.other.unit.suffix.integer",
                "entity.name.type",
                "punctuation.separator.dot.decimal",

                "entity.other.inherited-class.php",
                "support.function.magic.php",
                "keyword.type.cs",
                "storage.type.java"
            ],
            "settings": {
                "foreground": "#ee9c76",
            }
        },
        {
            "name": "Color 6",
            "scope": [
                "string",
                "punctuation.definition.string",
                "support.function.constructor",

                "storage.type.class.jsdoc",
                "keyword.other.phpdoc.php",
                "keyword.other.documentation.javadoc.java",
                "markup.inline.raw.string.markdown"
            ],
            "settings": {
                "foreground": "#82be8b",
            }
        },
        {
            "name": "Color 7 - Normal white; reset color",
            "scope": [
                "variable.other",
                "source.sql.embedded",
                "keyword.other.php",
                "support.type.property-name.css",
                "keyword.key.toml"
            ],
            "settings": {
                "foreground": "#d3d3d3"
            }
        },

        {
            "name": "Punctuation",
            "scope": [
                "punctuation.definition",
                "punctuation.section",
                "punctuation.terminator",
                "keyword.operator",
                "keyword.operator.type.annotation",
                "keyword.control.ternary",
                "punctuation.accessor",
                "meta.brace",
                "storage.type.function.arrow",
                "storage.modifier.reference",
                "storage.modifier.pointer",
                "punctuation.separator",
                "punctuation.brackets",
                "punctuation.semi",
                "punctuation.comma",
                "punctuation.curlybrace",
                "punctuation.parenthesis",
                "punctuation.bracket.round",
                "punctuation.bracket.angle",

                "constant.language.import-export-all.ts",
                "keyword.operator.type.ts",
                "punctuation.brackets.attribute.rust",
                "punctuation.brackets.round.rust",
                "meta.attribute.rust",
                "meta.interpolation.rust",
                "entity.name.function.attribute.rust",
                "meta.preprocessor.cs"
            ],
            "settings": {
                "foreground": "#a3a3a3",
            }
        },
        {
            "name": "Comments",
            "scope": [
                "comment",
                "punctuation.definition.comment"
            ],
            "settings": {
                "foreground": "#7e7e7e"
            }
        }
    ]
}
