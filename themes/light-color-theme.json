// https://code.visualstudio.com/api/references/theme-color

{
    "name": " Valatos Theme Light",
    "type": "light",
    "colors": {
        "focusBorder": "#c2436d",
        "foreground": "#272727",

        "selection.background": "#c2436d5b",

        "editor.background": "#ececec",
        "editor.foreground": "#272727",

        "editor.findMatchBackground": "#88f09142",
        "editor.findMatchHighlightBackground": "#e47d704d",

        "editor.selectionBackground": "#ec6d975b",
        "minimap.selectionHighlight": "#ec6d975b",

        "textLink.foreground": "#c2436d",
        "textLink.activeForeground": "#943253",

        "statusBar.background": "#adadad",
        "statusBar.noFolderBackground": "#adadad",

        "button.background": "#c2436db6",
        "button.foreground": "#ececec",
        "button.hoverBackground": "#c2436dd0",

        "dropdown.background": "#f0f0f0",
        "dropdown.foreground": "#5c5c5c",
        "dropdown.border": "#bebebe",
        "dropdown.listBackground": "#f0f0f0",

        "activityBar.background": "#e4e4e4",
        "activityBar.foreground": "#646464",
        "activityBar.border": "#adadad",
        "activityBar.activeBorder": "#c2436d",

        "activityBarBadge.background": "#c2436d",
        "activityBarBadge.foreground": "#d3d3d3",

        "sideBar.background": "#dbdbdb",
        "sideBar.border": "#bebebe",
        "sideBar.foreground": "#5e5e5e",
        "sideBar.dropBackground": "#a3a3a33f",

        // "sideBarTitle.foreground":bbbbbb777",

        "sideBarSectionHeader.background": "#c2c2c2",
        "sideBarSectionHeader.border": "#bebebe",
        // "sideBarSectionHeader.foreground": "#d3d3d3",

        "editorGroup.border": "#454545",
        "editorGroup.dropBackground": "#a3a3a325",
        "editorGroup.focusedEmptyBorder": "#c2436d",

        "editorGroupHeader.border": "#bebebe",
        "editorGroupHeader.noTabsBackground": "#dddddd",
        "editorGroupHeader.tabsBackground": "#dddddd",

        "list.hoverBackground": "#c2436d2c",
        "list.activeSelectionBackground": "#bebebe",
        "list.focusOutline": "#c5c5c5",
        "list.focusBackground": "#c2436d5b",
        "list.highlightForeground": "#c2436d",

        "pickerGroup.foreground": "#c2436d",

        "tab.activeBackground": "#ececec",
        "tab.activeBorder": "#c2436d",
        // "tab.activeForeground": "#d3d3d3",
        "tab.inactiveBackground": "#e2e2e2",
        "tab.hoverBackground": "#dddddd",

        "panel.background": "#dbdbdb",
        "panel.border": "#bebebe",

        "settings.modifiedItemIndicator": "#c2436d",

        "checkbox.background": "#d3d3d3",

        "progressBar.background": "#c2436d",

        "titleBar.activeBackground": "#e4e4e4",
        "titleBar.inactiveBackground": "#e4e4e4",

        "editor.hoverHighlightBackground": "#c2436d27"
    },
    "tokenColors": [
        {
            "name": "Color 1",
            "scope": [
                "storage.type",
                "storage.modifier",
                "keyword.operator.type",
                "keyword.operator.new",
                "keyword.operator.delete",
                "keyword.control",
                "keyword.other",
                "keyword.operator.expression",
                "entity.name.tag",

                // "entity.name.tag.html",
                // "entity.name.tag.css",
                "keyword.operator.logical.php",
                "keyword.cmake",
                "support.type.property-name.json",
                "entity.name.section.markdown",
                "meta.tag.table.toml"
            ],
            "settings": {
                "foreground": "#e44379",
            }
        },
        {
            "name": "Color 2",
            "scope": [
                "keyword.operator.sizeof",
                "entity.other.inherited-class",
                "entity.name.scope-resolution",
                "entity.name.other.preprocessor.macro",
                "storage.type.annotation",
                "entity.name.namespace",
                "storage.type.generic",
                "constant.character.escape",

                "entity.other.attribute-name",
                "entity.source.cmake",
                "meta.property-name.css",
                "support.type.property-name.css",
                "constant.other.class.php",
                "markup.underline.link.markdown",
                "keyword.key.toml"
            ],
            "settings": {
                "foreground": "#9276eb",
            }
        },
        {
            "name": "Color 3",
            "scope": [
                "variable.other.object",

                "meta.property-value.css",
                "punctuation.definition.constant.css",
                "punctuation.section.embedded.begin.php",
                "punctuation.section.embedded.end.php",
                "variable.other.global.php",
                "variable.other.global.safer.php",
                "entity.name.function.macro.rust"
            ],
            "settings": {
                "foreground": "#3394e2"
            }
        },
        {
            "name": "Color 4",
            "scope": [
                "meta.function-call",
                "entity.name.function",
                "variable.language.this",

                "meta.selector.css",
                "meta.function.url.css",
                "support.other.namespace.php",
                "support.function.construct.output.php",
                "keyword.other.type.php",
                "storage.modifier.package.java",
                "storage.modifier.import.java",
                "storage.source.cmake",
                "markup.bold.markdown",
                "markup.italic.markdown",
                "variable.source.cmake",
                "variable.language.documentroot.xml"
            ],
            "settings": {
                "foreground": "#506f9e",
            }
        },
        {
            "name": "Color 5",
            "scope": [
                "constant.numeric",
                "constant.language",
                "support.type",
                "support.class",
                "support.type.builtin",
                "support.type.primitive",
                "storage.type.built-in",
                "storage.type.built-in.primitive",
                "keyword.other.unit.suffix.integer",
                "entity.name.type",

                "entity.other.inherited-class.php",
                "support.function.magic.php",
                "keyword.type.cs",
                "storage.type.java"
            ],
            "settings": {
                "foreground": "#e9672b",
            }
        },
        {
            "name": "Color 6",
            "scope": [
                "string",
                "punctuation.definition.string",
                "support.function.constructor",
                "variable.language.self",

                "storage.type.class.jsdoc",
                "keyword.other.phpdoc.php",
                "keyword.other.documentation.javadoc.java",
                "markup.inline.raw.string.markdown"
            ],
            "settings": {
                "foreground": "#40a750",
            }
        },
        {
            "name": "Color 7 - Normal black; reset color",
            "scope": [
                "keyword.other.php",
                "keyword.other.sql",
                "source.sql.embedded"
            ],
            "settings": {
                "foreground": "#272727"
            }
        },

        {
            "name": "Punctuation",
            "scope": [
                "punctuation.definition",
                "punctuation.section",
                "punctuation.terminator",
                "keyword.operator",
                "keyword.operator.type.annotation",
                "keyword.control.ternary",
                "punctuation.accessor",
                "meta.brace",
                "storage.type.function.arrow",
                "storage.modifier.reference",
                "storage.modifier.pointer",
                "punctuation.separator",
                "punctuation.brackets",
                "punctuation.semi",
                "punctuation.comma",
                "punctuation.curlybrace",
                "punctuation.parenthesis",
                "punctuation.bracket.round",
                "punctuation.bracket.angle",

                "keyword.operator.type.ts",
                "punctuation.brackets.attribute.rust",
                "punctuation.brackets.round.rust",
                "meta.attribute.rust",
                "meta.interpolation.rust",
                "meta.preprocessor.cs"
            ],
            "settings": {
                "foreground": "#929292",
            }
        },
        {
            "name": "Comments",
            "scope": [
                "comment",
                "punctuation.definition.comment"
            ],
            "settings": {
                "foreground": "#555555",
            }
        }
    ]
}
